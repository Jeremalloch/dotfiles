# Git aliases:
alias gtkk='git lg -10'
alias gau='git add -u'
alias gaa='git add --all'
alias gst='git status'
alias gp='git push'
alias gd='git diff'
alias gb='git branch | cat'
alias gcm='git checkout master'
alias gca='git commit --amend'

# Unix
alias ll='ls -l'

# Apple Bazel
alias bmac='bazel build --config=darwin-x86_64 //pdk/vintner/...'
alias tmac='bazel test --config=darwin-x86_64 //pdk/vintner/...'
